{
  "name": "vortex-workspace/framework",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
    "framework",
    "vortex"
  ],
  "authors": [
    {
      "name": "Caio Albuquerque",
      "email": "abcv.tenorio@gmail.com"
    }
  ],
  "require": {
    "php": "~8.3.0",
    "ext-ctype": "*",
    "ext-intl": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-simplexml": "*",
    "aws/aws-sdk-php": "^3.306",
    "doctrine/inflector": "~2.0.10",
    "guzzlehttp/guzzle": "~7.8.0",
    "monolog/monolog": "~3.6.0",
    "nesbot/carbon": "~2.72.3",
    "ramsey/uuid": "~4.7.6",
    "robinvdvleuten/ulid": "~v5.0.0",
    "spatie/regex": "~3.1.1",
    "symfony/console": "~v7.0.7",
    "vlucas/phpdotenv": "~v5.6.0",
    "vortex-workspace/cosmo": "dev-feature/COS-2-implement-prompt-helpers",
    "ext-curl": "*",
    "ext-dom": "*"
  },
  "require-dev": {
    "composer/composer": "*",
    "symfony/var-dumper": "~7.0.7",
    "fakerphp/faker": "~v1.23.1",
    "phpunit/phpunit": "~10.5.20"
  },
  "autoload": {
    "psr-4": {
      "Core\\": "src/Core/",
      "Stellar\\": "src/Stellar/"
    }
  },
  "autoload-dev": {
    "files": [
      "src/Stellar/Alias/main.php"
    ],
    "psr-4": {
      "Stellar\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Stellar\\Composer\\Package::autoload"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
